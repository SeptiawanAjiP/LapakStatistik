<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.septiawanaji.myapplication"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ajipradana.lapak.UserInterface.SplashScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ajipradana.lapak.UserInterface.KategoriBarang" />
        <activity android:name="com.example.ajipradana.lapak.UserInterface.DetailBarang" />
        <activity android:name="com.example.ajipradana.lapak.Notifikasi.Main" />
        <activity android:name="com.example.ajipradana.lapak.UserInterface.PilihanKategori" />
        <activity android:name="com.example.ajipradana.lapak.UserInterface.DaftarUser" />
        <activity
            android:name="com.example.ajipradana.lapak.UserInterface.Navigation"
            android:theme="@style/AppTheme.NoActionBar" />

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.ajipradana.lapak.Notifikasi.Main" />

        <activity android:name="com.example.ajipradana.lapak.Notifikasi.TestActivity" />
    </application>

</manifest>